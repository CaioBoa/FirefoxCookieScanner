  {
    "manifest_version": 2,
    "name": "Caio Supreme Cookie Inspector Hacker Killer 3000",
    "version": "1.0",
    "permissions": [
      "cookies",
      "storage",
      "activeTab",
      "webRequest",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    }
  }
  